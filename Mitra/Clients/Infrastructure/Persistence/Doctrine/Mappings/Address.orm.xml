<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Mitra\Address" table="Address">
    <id name="id" type="binary" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idClient" type="binary" column="id_client" nullable="false">
      <options>
        <option name="default"/>
      </options>
    </field>
    <field name="postalcode" type="smallint" column="postalCode" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="province" type="string" column="province" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="isactive" type="boolean" column="isActive" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
